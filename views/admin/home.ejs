<%- include('../layouts/header.ejs')%>

<div class="container">
  <h1 id="home-h1">Welcome <%=admin.name%></h1>
  <table
    style="
      border-collapse: collapse;
      width: 50%;
      margin: auto;
      border: 1px solid #ddd;
    "
  >
    <tr>
      <td style="border: 1px solid rgb(60, 46, 20); padding: 8px">Name</td>
      <td style="border: 1px solid rgb(60, 46, 20); padding: 8px">
        <%=admin.name%>
      </td>
    </tr>
    <tr>
      <td style="border: 1px solid rgb(60, 46, 20); padding: 8px">Email</td>
      <td style="border: 1px solid rgb(60, 46, 20); padding: 8px">
        <%=admin.email%>
      </td>
    </tr>
  </table>
  <br />
  <a id="home-a" href="/admin/dashboard">Go to Dashboard</a>
  <a id="home-a" href="/admin/logout">Logout</a>
</div>

<%-include('../layouts/footer.ejs')%>
